$include /etc/inputrc

TAB: menu-complete
set show-all-if-ambiguous on
set show-all-if-unmodified on
set colored-stats on
set match-hidden-files off
#set completion-ignore-case on

"\C-[[A": history-search-backward
"\C-[[B": history-search-forward

"\M-o": "\C-p\C-a\M-f "
$if Bash
	 Space: magic-space
$endif
